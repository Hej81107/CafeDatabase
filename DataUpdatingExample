Payment:
UPDATE PAYMENT
SET Total = '30.00'
WHERE Cus_ID = 005;

UPDATE PAYMENT
SET Order_Date = '2023-05-22' 
WHERE Cus_ID = 009 and Order_Time = '10:19 AM';
Item:
UPDATE ITEM
SET Item_Quantity = 7
WHERE Cafe_Num = 5;

UPDATE ITEM
SET Description = 'Yummy Muffin'
WHERE Item_Name = 'Muffin';


Order:
UPDATE ORDER_
SET Time_recieved = '3:30 PM' and tim_completed = '4:00 PM'
WHERE Order_Num = 1003;

UPDATE ORDER_
SET Time_received = '5:00 AM'
WHERE time_complete= '4:00 AM' and Item_Num= 5;
